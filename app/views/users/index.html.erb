<div class="index-page">
  <div class="container">
    <div class="row">
      <div class="col-xs-3">

<%= render 'users/list', users: @users %>

      </div>
      <div class="col-xs-9">
          <h2>Users</h2>

          <table class="table table-striped"">
            <tr>
            <th>image</th>
            <th>name</th>
            </tr>
          <% @users.each do |user| %>
          <tr>
            <td><%= "image" %></td>
            <td><%= user.name %></td>
            <td><%= link_to "show", user_path(user.id) %></td>
          </tr>
          <% end %>
          </table>
      </div>
    </div>
  </div>
</div>